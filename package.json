{
  "name": "sandbox-subgraph",
  "version": "0.1.0",
  "scripts": {
    "create": "graph create pixowl/the-sandbox --node https://api.thegraph.com/deploy/",
    "create-local": "graph create pixowl/the-sandbox --node http://127.0.0.1:8020",
    "codegen": "graph codegen",
    "build": "graph build",
    "generate": "node scripts/generate.js",
    "deploy": "dotenv -- cross-var graph deploy --product hosted-service",
    "deploy:mainnet": "yarn generate mainnet && yarn codegen && yarn deploy pixowl/the-sandbox",
    "deploy:rinkeby": "yarn generate rinkeby && yarn codegen && yarn deploy pixowl/the-sandbox-rinkeby",
    "deploy:goerli": "yarn generate goerli && yarn codegen && yarn deploy sandboxthegraph/the-sandbox-goerli",
    "deploy-local": "graph deploy pixowl/the-sandbox --ipfs http://localhost:5001 --node http://127.0.0.1:8020"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.26.0",
    "@graphprotocol/graph-ts": "^0.24.1",
    "cross-var": "^1.1.0",
    "dotenv-cli": "^5.0.0",
    "fs-extra": "^10.0.0",
    "handlebars": "^4.7.7",
    "rimraf": "^3.0.2"
  },
  "dependencies": {}
}
