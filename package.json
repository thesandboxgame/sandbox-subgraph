{
  "name": "sandbox-subgraph",
  "version": "0.1.0",
  "scripts": {
    "create": "graph create pixowl/the-sandbox --node https://api.thegraph.com/deploy/",
    "create-local": "graph create pixowl/the-sandbox --node http://127.0.0.1:8020",
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "dotenv -- cross-var graph deploy --access-token $THEGRAPH_TOKEN pixowl/the-sandbox --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "deploy-local": "graph deploy pixowl/the-sandbox --ipfs http://localhost:5001 --node http://127.0.0.1:8020"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.20.1",
    "@graphprotocol/graph-ts": "^0.20.0",
    "cross-var": "^1.1.0",
    "dotenv-cli": "^4.0.0",
    "fs-extra": "^9.0.0",
    "handlebars": "^4.7.6",
    "rimraf": "^3.0.2"
  },
  "dependencies": {}
}
